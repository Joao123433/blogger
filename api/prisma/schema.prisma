generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Posts {
  id           String    @id
  title        String
  introduction String
  story        String
  conclusion   String
  created_at   DateTime? @default(now())
  updated_at   DateTime? @default(now())
}
